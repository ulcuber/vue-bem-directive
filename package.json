    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-transform-object-rest-spread": "^7.0.0-beta.3",
    "chai": "^4.2.0",
    "codacy-coverage": "^3.4.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-flowtype": "^3.2.1",
